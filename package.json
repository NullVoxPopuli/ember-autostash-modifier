{
  "name": "ember-autostash-modifier",
  "version": "0.1.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/NullVoxPopuli/ember-autostash-modifier",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:docs": "yarn build && mv dist docs",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:try-one": "ember try:one",
    "test:all": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "6.0.1",
    "ember-cli-typescript": "5.0.0",
    "ember-modifier": "3.2.1",
    "ember-template-lint": "4.3.0"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.6.0",
    "@ember/test-waiters": "3.0.1",
    "@embroider/compat": "1.5.0",
    "@embroider/test-setup": "1.5.0",
    "@glimmer/component": "1.1.0",
    "@glimmer/tracking": "1.1.0",
    "@nullvoxpopuli/eslint-configs": "2.2.4",
    "@types/ember": "4.0.0",
    "@types/ember-qunit": "5.0.0",
    "@types/ember__test-helpers": "2.6.1",
    "@types/qunit": "2.11.3",
    "@types/rsvp": "4.0.4",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "2.4.1",
    "ember-cli": "4.2.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-code-snippet": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-prism": "0.12.0",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "4.3.0",
    "ember-source-channel-url": "3.0.0",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-decorator-position": "4.0.1",
    "eslint-plugin-ember": "10.5.9",
    "eslint-plugin-node": "11.1.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "qunit": "2.18.1",
    "qunit-dom": "2.0.0",
    "semantic-release": "19.0.2",
    "typescript": "4.6.3",
    "webpack": "5.70.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "16.14.2",
    "yarn": "1.22.18"
  }
}
